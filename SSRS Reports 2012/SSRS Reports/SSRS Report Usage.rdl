  <Description>Shows who has run a report and how often</Description>
  <Author>Ryan Duclos, Mike Gordon</Author>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="Reports">
      <DataSourceReference>Reports</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>54720dcb-f8f6-48bd-8f92-5b5fc1e46651</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="ReportHistory">
      <Query>
        <DataSourceName>Reports</DataSourceName>
        <CommandText>SELECT     SUBSTRING(c.Path, 2, LEN(c.Path)-1  - CHARINDEX('/', REVERSE(c.Path))) AS Folder, c.Name as ReportName, el.UserName
,  isnull(SGid.FirstName + ' ' + SGid.Surname, ' ') as Person
, el.TimeStart AS ReportTime, DATEADD(dd, DATEDIFF(dd, '2000-01-01', el.TimeStart), '2000-01-01') AS ReportDate, DATEPART(hh, el.TimeStart) AS ReportHour, el.Format, 
                      REPLACE(REPLACE(CAST(el.Parameters AS nvarchar(2048)), '%26', ''), '&amp;', CHAR(10) + CHAR(13)) AS ParamText
FROM         SGid RIGHT OUTER JOIN
                      ExecutionLog AS el ON 
SGid.sgid =  SUBSTRING(el.UserName, 4, 12)
 RIGHT OUTER JOIN
                      SecData AS sd INNER JOIN
                      Catalog AS c ON sd.PolicyID = c.PolicyID ON el.ReportID = c.ItemID
WHERE     (c.Type = 2) AND (c.Hidden = 0)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Folder">
          <DataField>Folder</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ReportName">
          <DataField>ReportName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UserName">
          <DataField>UserName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ReportTime">
          <DataField>ReportTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ReportDate">
          <DataField>ReportDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ReportHour">
          <DataField>ReportHour</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Format">
          <DataField>Format</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ParamText">
          <DataField>ParamText</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Person">
          <DataField>Person</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
  <Code>Shared Function SetColumn(ByVal row As Integer, ByVal column As Integer) As String
  While row &gt; column
    If column &gt;= row Then Exit While
    row = row - column
  End While

  Return row
End Function

Shared Function SetRow(ByVal row As Integer, ByVal column As Integer) As String
  Dim i As Integer = 1

  While row &gt; column
    i = i + 1
    If column &gt;= row Then Exit While
    row = row - column
  End While

  Return i
End Function</Code>
