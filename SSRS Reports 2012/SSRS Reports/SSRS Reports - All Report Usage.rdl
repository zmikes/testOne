  <DataSets>
    <DataSet Name="Reports">
      <Query>
        <DataSourceName>Reports</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Folder">
            <Value>=Parameters!Folder.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        SUBSTRING(c.Path, 1, LEN(c.Path) - CHARINDEX('/', REVERSE(c.Path))) AS Folder, c.Name, el.UserName, el.TimeStart AS ReportTime, DATEADD(dd, DATEDIFF(dd, 
                         '2000-01-01', el.TimeStart), '2000-01-01') AS ReportDate, DATEPART(hh, el.TimeStart) AS ReportHour, el.Format, 
                         REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(CAST(el.Parameters AS nvarchar(2048)), '%26', ''), '&amp;', CHAR(10) + CHAR(13)), '%2F', '/'), '%20', ' '), '%3A', ':') 
                         AS ParamText, ISNULL(SGid.Surname, el.UserName) AS Surname, SGid.FirstName, Modified.FirstName + ' ' + Modified.Surname AS ModifiedBy, 
                         c.ModifiedDate
FROM            Users LEFT OUTER JOIN
                         SGid AS Modified ON SUBSTRING(Users.UserName, 4, 12) = Modified.SGid RIGHT OUTER JOIN
                         SecData AS sd INNER JOIN
                         Catalog AS c ON sd.PolicyID = c.PolicyID ON Users.UserID = c.ModifiedByID LEFT OUTER JOIN
                         SGid RIGHT OUTER JOIN
                         ExecutionLog AS el ON SGid.SGid = SUBSTRING(el.UserName, 4, 12) ON c.ItemID = el.ReportID
WHERE        (c.Type = 2) AND (c.Hidden = 0) AND (SUBSTRING(c.Path, 1, LEN(c.Path) - CHARINDEX('/', REVERSE(c.Path))) LIKE '%' + @Folder + '%')
ORDER BY Folder, c.Name, Surname, ReportTime</CommandText>
      </Query>
